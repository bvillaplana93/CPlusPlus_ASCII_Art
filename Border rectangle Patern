    int width = 50;
    int height = 10;
    int borderW = 2;
    int borderH = 2;
    
   //How to make a rectangle patern
    
     for(int i = 0; i<height; i++)
    {
        for(int a = 0; a<width; a ++)
        {

            if((i+a) % 2 == 0)
            {
               cout<<"#";
           
            } 
            else
            {
            
               cout<<" ";
            
            }

        }

            cout<<""<<endl;
    }
    
